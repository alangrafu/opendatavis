PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX : <http://opendatav.is/vocab/>

SELECT ?paramX ?paramY ?sort WHERE{
  GRAPH <{{uri}}>{
    <{{uri}}> :hasParameter ?p1, ?p2 ;
              :sortedBy ?sort .
    ?p1 :parameterName "x";
       :parameterValue ?paramX .    
    ?p2 :parameterName "y";
       :parameterValue ?paramY .    
  }
}
