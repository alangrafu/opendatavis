PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX : <http://opendatav.is/vocab/>

SELECT ?paramX ?paramY WHERE{
  GRAPH <{{uri}}>{
    <{{uri}}> :hasParameter ?p1, ?p2 .
    ?p1 :parameterName "lon";
       :parameterValue ?paramX .    
    ?p2 :parameterName "lat";
       :parameterValue ?paramY .    
  }
}
